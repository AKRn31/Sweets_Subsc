<h1>サブスク一覧</h1>
  <p><%= link_to "サブスク登録",  new_admin_subsc_path %> </p>
 

  <div>
    <% @subscs.each do |subsc| %>
      
        <%= link_to admin_subsc_path(subsc.id) do %>
          <p><%= subsc.company_name %></p>
          <p><% if subsc.subsc_image.present? %>
            <%= image_tag subsc.subsc_image, size: "200x200" %></p>
          <% end %>
          <p><%= subsc.introduction %></p>
        
      <% end %>
     
   <% end %>
  </div>

