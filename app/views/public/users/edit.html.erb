<h1>ユーザー情報の編集画面</h1>
<%= form_with model: @user do |f| %>
  <label for="inputName">Name</label>
  <%= f.text_field :username, autofocus: true, id:"inputName", placeholder:"名前"%>
  <label for="inputEmail">Email</label>
  <%= f.text_field :email, autofocus: true, id:"inputEmail", placeholder:"メールアドレス"%>
  <%= f.submit "変更を保存" %>
  
<% end %>
<%= link_to "退会", users_confilm_path %>