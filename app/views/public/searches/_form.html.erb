
<div class="d-flex justify-content-end">
    <%= form_with url: search_path, method: :get, local: true,class:"form-inline my-2 my-lg-0" do |f| %>
      <%= f.text_field :content, placeholder: "検索", class:"form-control mr-sm-2" %>
      <div class="form-group">

          <%= f.select :model, options_for_select({ "User" => "user", "Subsc" => "subsc" }) %>
          <%= f.select :method, options_for_select({ "完全一致" => "perfect", "前方一致" => "forward", "後方一致" => "backward", "部分一致" => "partial" }), class:"dropdown-menu" %>
       </div>
      <button class="btn btn-outline-success " type="submit">検索</button>
    <% end %>

</div>
